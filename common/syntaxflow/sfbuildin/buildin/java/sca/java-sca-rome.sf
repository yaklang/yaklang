desc(
    title: 'SCA: Checking Java rome Existed Vulnerabilities',
    type: vuln,
    risk:sca,
)

__dependency__.*rome.version as $ver;
$ver in (,1.15.0] as $vuln_rome;
alert $vuln_rome for {
    message: 'SCA: rome versions between 1 and 1.15.0 have an XXE vulnerability',
    cve: 'CVE-2020-15250',
    level: 'high',
}
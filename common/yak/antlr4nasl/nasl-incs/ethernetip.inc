#
# Return the Ethernet/IP device type
#
function ethip_get_device_type(code)
{
  foreach type (keys(device_types)) {
    if (code == type)
      return device_types[type];
  }
  return "Unknown Device Type";
}


#
# Return the Ethernet/IP vendor name
#
function ethip_get_vendor_name(code)
{
  if (code < max_index(vendors))
    return vendors[code];
  else
    return "Unknown vendor";
}

ctx, cancel = context.WithCancel(context.Background())
swg = sync.NewSizedWaitGroup(20)
getRememberMeNumber = (rsp,key)=>{
    newrsp = str.Replace(rsp,key,"", -1)
    return (len(rsp)-len(newrsp))/len(key)
}

